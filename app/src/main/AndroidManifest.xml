<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androidapp1d">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Stud.Feed.StudFeedActivity" />
        <activity
            android:name=".Stud.StudNotificationActivity"
            android:parentActivityName=".Stud.Feed.StudFeedActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Stud.Feed.StudFeedActivity" />
        </activity>
        <activity android:name=".Stud.Booking.StudBookingActivity"></activity>
        <activity android:name=".Stud.Profile.StudProfileActivity"></activity>
        <activity android:name=".Prof.ProfFeedActivity"></activity>
        <activity android:name=".Stud.StudSearchActivity"
            android:windowSoftInputMode="adjustPan"/>

        <activity android:name=".Stud.Booking.StudBookingProf"></activity>
        <activity android:name=".Stud.Booking.StudBookingMod"></activity>
        <activity android:name=".Stud.Booking.StudBookingUpcoming"></activity>
        <activity android:name=".Stud.Booking.StudBookingPrevious"></activity>
        <activity android:name=".Stud.Booking.StudBookingFavourites"></activity>
        <activity android:name=".Stud.Booking.StudProfDetails"></activity>
        <activity android:name=".Stud.Booking.StudBookingProfSlot"></activity>

        <activity android:name=".Prof.ProfBookingActivity" />
        <activity android:name=".Prof.ProfNotificationActivity" />
        <activity android:name=".Prof.ProfProfileActivity" />
        <activity android:name=".Prof.ProfSearchActivity"></activity>

    </application>

</manifest>